{
  "name": "@pos-saas/sections",
  "version": "1.0.0",
  "description": "Reusable restaurant website section components",
  "private": false,
  "main": "index.ts",
  "types": "index.ts",
  "exports": {
    ".": {
      "import": "./index.ts",
      "types": "./index.ts"
    },
    "./hero": {
      "import": "./hero.tsx",
      "types": "./hero.tsx"
    },
    "./featured-items": {
      "import": "./featured-items.tsx",
      "types": "./featured-items.tsx"
    },
    "./why-choose-us": {
      "import": "./why-choose-us.tsx",
      "types": "./why-choose-us.tsx"
    },
    "./info-cards": {
      "import": "./info-cards.tsx",
      "types": "./info-cards.tsx"
    },
    "./cta": {
      "import": "./cta.tsx",
      "types": "./cta.tsx"
    },
    "./testimonials": {
      "import": "./testimonials.tsx",
      "types": "./testimonials.tsx"
    }
  },
  "keywords": [
    "react",
    "components",
    "restaurant",
    "website",
    "sections",
    "reusable",
    "typescript"
  ],
  "author": "POS SaaS Team",
  "license": "PROPRIETARY",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/pos-saas"
  },
  "bugs": {
    "url": "https://github.com/your-org/pos-saas/issues"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "typescript": ">=5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "changelog": {
    "1.0.0": {
      "date": "2025-12-26",
      "changes": [
        "Initial release",
        "Added Hero component",
        "Added FeaturedItems component",
        "Added WhyChooseUs component",
        "Added InfoCards component",
        "Added Cta component",
        "Added Testimonials component",
        "Full TypeScript support",
        "Dark mode support",
        "Responsive design",
        "Comprehensive documentation"
      ]
    }
  },
  "scripts": {
    "version:check": "node -e \"console.log(require('./package.json').version)\"",
    "version:list": "echo 'Current version: 1.0.0' && echo 'Components: 6 (Hero, FeaturedItems, WhyChooseUs, InfoCards, Cta, Testimonials)'"
  }
